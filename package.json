    "@types/bn.js": "^5.1.5",
    "typescript": "^5.5.2",
    "ts-node": "^10.9.1",
    "@types/node": "^17.0.8",
    "axios": "^1.7.2",
    "bs58": "^6.0.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.5",
    "ethers": "^5.7.2",
    "minimist": "^1.2.8",
    "prettier": "^3.3.2"
  }
