{
  "name": "soon-birdge-tool",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "index": "npx ts-node --project tsconfig.json src/index.ts",
    "init_soon": "npx ts-node --project tsconfig.json src/init_soon.ts",
    "reset_output_oracle": "npx ts-node --project tsconfig.json src/reset_output_oracle.ts",
    "expend_deposit": "npx ts-node --project tsconfig.json src/expend_deposit_account.ts",
    "issue_withdraw": "npx ts-node --project tsconfig.json src/issue_withdraw.ts",
    "propose_withdraw": "npx ts-node --project tsconfig.json src/propose_withdraw.ts",
    "fetch_proposed_height": "npx ts-node --project tsconfig.json src/fetch_proposed_height.ts",
    "prove_withdraw": "npx ts-node --project tsconfig.json src/prove_withdraw.ts",
    "fetch_withdraw_finalize_status": "npx ts-node --project tsconfig.json src/fetch_withdraw_finalize_status.ts",
    "finalize_withdraw": "npx ts-node --project tsconfig.json src/finalize_withdraw.ts",
    "deposit_eth": "npx ts-node --project tsconfig.json src/deposit_eth.ts",
    "fetch_all_withdrawal_tx_by_user": "npx ts-node --project tsconfig.json src/fetch_all_withdrawal_tx_by_user.ts",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "pkg": "yarn pkg_init_soon && yarn pkg_reset_output_oracle",
    "pkg_init_soon": "tsc src/init_soon.ts || pkg -t node18-linux-x64 src/init_soon.js --out-path bin",
    "pkg_reset_output_oracle": "tsc src/reset_output_oracle.ts || pkg -t node18-linux-x64 src/reset_output_oracle.js --out-path bin"
  },
  "keywords": [],
  "author": "",
